import array


x=array.array("i",[2,3,4,5,66,7])
x1=[]
for i in x:
    print(i)
    x1.append(i)
print(x1)
print(len(x))
print("التجربة الثانية")
x.append(900)
x2=[]
for i in x:
    print(i)
    x2.append(i)
print(x2)
print(len(x))
x2.sort()
print("ترتيب")
print(x2)
x2.reverse()
print("عكس")
print(x2)
